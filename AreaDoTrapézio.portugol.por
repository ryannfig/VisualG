Algoritmo "semnome"

Var
   // Seção de Declarações das variáveis
   BMA, BME, H, A: real

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   Escreva("Digite a base maior: ")
   Leia(BMA)
   Escreva("Digite a base menor: ")
   Leia(BME)
   Escreva("Digite a altura: ")
   Leia(H)
   A <- (BMA+BME)*H/2
   Escreva("A area do trapézio é: ", A)

Fimalgoritmo
